{
  "name": "nzsb-i-gen",
  "version": "1.0.13",
  "description": "SVG Icon Generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib/**/*",
    "/fonts/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "fantasticon \"src/icon-src\" -o fonts --name nzsb --prefix nzsb",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "nzsg-gen": "fantasticon \"src/icon-src\" -o fonts --name nzsb --prefix nzsb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanishkaisuru/Nzsb-i-Gen.git"
  },
  "keywords": [
    "nzsb"
  ],
  "author": "NZSB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kanishkaisuru/Nzsb-i-Gen/issues"
  },
  "homepage": "https://github.com/kanishkaisuru/Nzsb-i-Gen#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  }
}
